    "@nestjs/common": "^9.4.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.4.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-fastify": "^9.4.0",
    "@nestjs/platform-socket.io": "^9.4.2",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/websockets": "^9.4.2",
    "@prisma/client": "^5.0.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "csv-parse": "^5.3.10",
    "csv-stringify": "^6.4.0",
    "date-fns": "^2.30.0",
    "fast-csv": "^4.3.6",
    "fastify": "^4.17.0",
    "husky": "^8.0.3",
    "nodemailer": "^6.9.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pdfmake": "^0.2.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
